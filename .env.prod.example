# Production Environment Configuration
# Copy this file to .env.prod and fill in your actual values

# Application
APP_ENV=production
APP_DEBUG=false
LOG_LEVEL=info

# LangGraph Configuration
LANGGRAPH_API_URL=http://langgraph-api:8000
LANGGRAPH_API_KEY=TODO_ADD_YOUR_LANGGRAPH_API_KEY

# Database
DATABASE_URL=postgresql://user:password@postgres:5432/rag7_prod
REDIS_URL=redis://redis:6379/0

# n8n Configuration
N8N_WEBHOOK_URL=TODO_ADD_YOUR_N8N_WEBHOOK_URL
N8N_API_KEY=TODO_ADD_YOUR_N8N_API_KEY

# Provider Credentials
KIRO_AI_API_KEY=TODO_ADD_YOUR_KIRO_AI_API_KEY
KIRO_AI_BASE_URL=https://api.kiro.ai/v1

LINDY_AI_API_KEY=TODO_ADD_YOUR_LINDY_AI_API_KEY
LINDY_AI_BASE_URL=https://api.lindy.ai/v1

# Observability
SENTRY_DSN=TODO_ADD_YOUR_SENTRY_DSN
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# Security
API_SECRET_KEY=TODO_GENERATE_A_SECURE_RANDOM_SECRET_KEY
ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Kubernetes
K8S_NAMESPACE=default
K8S_SERVICE_ACCOUNT=rag7-service-account
