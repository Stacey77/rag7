{
  "name": "Main Orchestrator",
  "nodes": [
    {
      "parameters": {},
      "id": "start",
      "name": "Start",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "orchestrator",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 500],
      "webhookId": "orchestrator-webhook"
    },
    {
      "parameters": {
        "rules": {
          "rules": [
            {
              "operation": "contains",
              "value1": "={{ $json.pattern }}",
              "value2": "sequential"
            },
            {
              "operation": "contains",
              "value1": "={{ $json.pattern }}",
              "value2": "parallel"
            },
            {
              "operation": "contains",
              "value1": "={{ $json.pattern }}",
              "value2": "loop"
            }
          ]
        },
        "fallbackOutput": "extra"
      },
      "id": "router",
      "name": "Router",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 3,
      "position": [500, 400]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://langgraph-api:8000/api/v1/sequential",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "task",
              "value": "={{ $json.task }}"
            }
          ]
        },
        "options": {}
      },
      "id": "sequential",
      "name": "Sequential Pattern",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [750, 200]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://langgraph-api:8000/api/v1/parallel",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "task",
              "value": "={{ $json.task }}"
            }
          ]
        },
        "options": {}
      },
      "id": "parallel",
      "name": "Parallel Pattern",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [750, 400]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://langgraph-api:8000/api/v1/loop",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "task",
              "value": "={{ $json.task }}"
            },
            {
              "name": "quality_threshold",
              "value": "={{ $json.quality_threshold || 0.8 }}"
            }
          ]
        },
        "options": {}
      },
      "id": "loop",
      "name": "Loop Pattern",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [750, 600]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://langgraph-api:8000/api/v1/run",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "task",
              "value": "={{ $json.task }}"
            },
            {
              "name": "pattern",
              "value": "={{ $json.pattern }}"
            }
          ]
        },
        "options": {}
      },
      "id": "generic",
      "name": "Generic Pattern",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [750, 800]
    },
    {
      "parameters": {},
      "id": "merge",
      "name": "Merge",
      "type": "n8n-nodes-base.merge",
      "typeVersion": 2,
      "position": [1000, 400]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ $json }}"
      },
      "id": "respond",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1200, 400]
    }
